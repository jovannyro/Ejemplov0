@page
@model MyApp.Namespace.ListModel
@{
}
<div class="text-center center">
    <h1>Personas</h1>
</div>

<br />
<br/>
<br/>
<table class="table">
    <tr>
        <!-- <th>Id</th> -->
        <th>Nombre</th>
        <th>Edad</th>
        <th>Altura</th>
    </tr>
    @foreach (var persona in Model.Personas)
    {
        <tr>
            <!--<td>@persona.Id</td>-->
        <td>@persona.Nombre</td>
            <td>@persona.Edad</td>
            <td>@persona.Altura</td>
        </tr>
    }

</table>
